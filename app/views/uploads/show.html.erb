<div class="page-header">
  <%= link_to uploads_path, class: 'btn btn-primary' do %>
    All Uploads
  <% end %>
  <%= link_to edit_upload_path(@upload), class: 'btn btn-primary' do %>
    Edit
  <% end %>
</div>

<dl class="dl-horizontal">
  <dt>Title:</dt>
  <dd><%= @upload.title %></dd>
  <dt>Description:</dt>
  <dd><%= @upload.description %></dd>
  <dt>Files:</dt>
  <%= render 'file_list', upload: @upload %>
</dl>
